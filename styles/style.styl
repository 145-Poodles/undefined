body
	position relative
	font-family RobotoCondensed-Bold
	margin 0
	padding 0
	// background #912735
@font-face
	font-family RobotoCondensed-Regular
	src url("../fonts/RobotoCondensed-Regular.ttf")
@font-face
	font-family RobotoCondensed-Bold
	src url("../fonts/RobotoCondensed-Bold.ttf")
@font-face
	font-family Sans-Narrow-Regular
	src url("../fonts/PT_Sans-Narrow-Web-Regular.ttf")
@font-face
	font-family Sans-Narrow-Bold
	src url("../fonts/PT_Sans-Narrow-Web-Bold.ttf")
#main
	margin 0 auto
	padding 0 0 20px 0
	width 100%
	text-align center
	// background #912735
	#wrapper
		margin 0 auto
		width 760px
		// .divider
		// 	margin 0 auto
		// 	width 300px
		// 	height 2px
	h1
		margin 0 auto
		padding 5px 0
		width 490px
		font-family Sans-Narrow-Regular
		color #912735
		letter-spacing 2px
		word-spacing 2px
	p
		font-family Sans-Narrow-Regular
		line-height 25px
		letter-spacing 0.7px
		word-spacing 2px
		color black
		font-size 17px
		font-weight 600
#main:last-of-type
	background #912735
	padding 20px 0 20px 0
	#wrapper
		width 800px
	p, h1
		color white
#landing
	padding 40px 0 40px 0
	text-align center
	// background #912735
	img
		border 8px solid white
		border-left 80px solid white
		border-right 80px solid white
		border-radius 6px
		pointer-events none
header
	// width 100%
	// background url('../images/XYZ.png')
	// background-size 100%
	// background-repeat no-repeat
#navbar
	margin 0 auto
	width 600px
	background #912735
	border-radius 5px
	ul
		margin 0 0 50px 0
		padding 10px 0
		text-align center
		li
			display inline-block
			margin 0 15px
			list-style none
			a
				font-family Sans-Narrow-Regular
				padding 3px 20px 3px 20px
				text-decoration none
				color white
				word-spacing 2px
				letter-spacing 1px
				font-size 15px
				transition all 0.4s ease
				border-radius 3px
			a:hover
				background white
				color #912735
		li:first-of-type
			a
				border none
#darkness
	position fixed
	display none
	z-index 999
	width 100%
	height 100%vh
	text-align center
.large
	z-index 1000
	position fixed
	top 0
	bottom 0
	left 0
	right 0
	margin auto
	width 450px
	height 850px
.iphone-display-2
	z-index 1000
	position fixed
	top 0
	bottom 0
	left 0
	right 0
	margin auto
	width 376px
	height 676px
	background-image url("../images/iphone-display-2.jpg")
	background-size cover
	transition all 0.4s ease
.iphone-display-1
	z-index 1000
	position fixed
	top 0
	bottom 0
	left 0
	right 0
	margin auto
	width 376px
	height 676px
	background-image url("../images/iphone-display-1.jpg")
	background-size cover
	transition all 0.4s ease
.toggle
	z-index 1111
	position fixed
	top 0
	bottom -757px
	left 0
	right 0
	margin auto
	width 61px
	height 61px
	background #282D31
	border none
	border-radius 50%
	cursor pointer
	border 4px solid white
	transition all 0.3s ease
	-webkit-box-shadow: 0px 0px 132px -27px rgba(0,0,0,1);
	-moz-box-shadow: 0px 0px 132px -27px rgba(0,0,0,1);
	box-shadow: 0px 0px 132px -27px rgba(0,0,0,1);
.flick
	background white
.toggle:hover
	background white
	border 4px solid #282D31
#lightbox
	display none
	z-index 1111
	position fixed
	width 100%
	height 100%vh
	background rgba(0,0,0,0.8)
	pointer-events none
#lightbox button, img, div
	pointer-events auto
