.show, .hide
	position fixed
	cursor pointer
	font-size 30px
	color black
	padding 0px 10px
.hide
	display none
	padding 5px 10px
	color white
header
	text-align center
header img
	width 370px
	pointer-events none
main 
	height 400px
main #wallpapers
	text-align center;
	#box p
		font-size 16px
	h1
		margin 0 auto
		padding 5px 0
		width 490px
		font-family Sans-Narrow-Regular
		color #912735
		letter-spacing 1px
		word-spacing 2px
	p
		margin 0 auto
		padding 15px 0 50px 0
		width 700px
		font-family Sans-Narrow-Regular
		line-height 25px
		letter-spacing 0.7px
		word-spacing 2px
		color black
		font-size 18px
		font-weight 600
	font-size 14px
	#box
		position relative
		display inline-block
		width 300px
		height 500px
		img
			margin 20px 0 20px 0
			width 150px
			transition all 0.3s ease
			cursor pointer
		span
			font-family Sans-Narrow-Regular
			display none
			position absolute
			margin-left auto
			margin-right auto
			left 0
			right 0
			top 40%
			text-transform uppercase
			font-size 13px
			letter-spacing 1px
			color white
			pointer-events none
		h2
			margin 5px 0
			color #8d2130
			text-transform uppercase
			letter-spacing 1px
			font-size 19px
		p
			width 300px;
			margin 5px 0
			padding 0
			letter-spacing 1px
		button
			font-family Sans-Narrow-Bold
			margin 10px 0 0 0
			background #8d2130;
			border none
			text-transform uppercase
			color white
			font-size 18px
			border 2px solid #8d2130
			border-radius 4px
			transition all 0.4s ease
			cursor pointer
			letter-spacing 2px
		button:hover
			background #EEEEEE;
			color #8d2130
footer
	padding 0 0 20px 0
	width 100%
	background #912735
	height 80px
	text-align center
	a
		width 80px
		height 80px
	.fb
		margin 0 auto
		padding 15px 0 0 0
		cursor pointer
		transition all 0.3s ease
	.fb:hover
		opacity 0.3
button:focus {
    outline: 0;
}
::-moz-selection { background: yellow; }
::selection { background: #EEEEEE; }